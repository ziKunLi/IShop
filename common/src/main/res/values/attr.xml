<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SatelliteMenu">
        <attr name="location">
            <enum name="left_bottom" value="0"/>
            <enum name="left_top" value="1"/>
            <enum name="right_bottom" value="2"/>
            <enum name="right_top" value="3"/>
        </attr>
        <attr name="radius" format="dimension"/>
    </declare-styleable>

    <!--AutoPhotoLayout-->
    <declare-styleable name="camera_flow_layout">
        <attr name="max_count" format="integer" />
        <attr name="item_margin" format="integer" />
        <attr name="line_count" format="integer" />
        <attr name="icon_size" format="dimension" />
    </declare-styleable>
</resources>